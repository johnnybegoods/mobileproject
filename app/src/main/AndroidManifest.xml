<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobile.utfpr.com.br.mobileproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Adicionar_Horas_Activity"
            android:parentActivityName=".PrincipalActivity" />
        <activity
            android:name=".ConfigActivity"
            android:parentActivityName=".PrincipalActivity" />
        <activity
            android:name=".ActivityAbout"
            android:parentActivityName=".PrincipalActivity" />
        <activity android:name=".Activity_VerHoras" />
    </application>

</manifest>